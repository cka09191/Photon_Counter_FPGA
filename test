T_1 ;
    %wait E_0000018792f3a240;
    %load/vec4 v0000018792f23f60_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 16;
    %assign/vec4 v000001879304df90_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0000018792f33c60_0;
    %flag_set/vec4 8;
