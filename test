    %addi 1, 0, 8;
    %assign/vec4 v0x129f1fec0_0, 0;
    %load/vec4 v0x129f1fec0_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 8;
    %cmp/u;
    %jmp/1 T_2.2, 6;
