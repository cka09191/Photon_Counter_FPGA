    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x129f089d0_0, 0, 1;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x129f1fec0_0, 0;
    %jmp T_2.5;
T_2.5 ;
    %pop/vec4 1;
T_2.0 ;
    %jmp T_2;
    .thread T_2;
    .scope S_0x129f04fd0;
T_3 ;
