    %load/vec4 v000001879304df90_0;
    %addi 1, 0, 16;
    %assign/vec4 v000001879304df90_0, 0;
T_1.2 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_00000187930417f0;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0000018792f7d100_0, 0, 1;
    %pushi/vec4 0, 0, 1;
